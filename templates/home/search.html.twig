<section class="home-map-search js-home-map-search" style="text-align: center;bottom: 40px;">
    <section class="benefits-section" id="benefits-section">
        <div class="container" style="    margin-top: -110px;
    background-color: white;
    height: 100%;
    padding-top: 65px; padding-bottom: 30px;">
            <div class="row">
                <div class="col-xs-6 col-sm-4">
                    <div class="benefit">
                        <div class="image-holder">
                            <img src="{{ 'global.images_url'|trans ~ asset('images/benefits-06.png') }}">
                        </div>
                        <span>{{ 'page.home.benefits.picto1'|trans|raw }}</span>
                    </div>
                </div>
                <div class="col-xs-6 col-sm-4">
                    <div class="benefit">
                        <div class="image-holder">
                            <img src="{{ 'global.images_url'|trans ~ asset('images/benefits-07.png') }}">
                        </div>
                        <span>{{ 'page.home.benefits.picto2'|trans|raw }}</span>
                    </div>
                </div>
                <div class="col-xs-6 col-sm-4">
                    <div class="benefit">
                        <div class="image-holder">
                            <img src="{{ 'global.images_url'|trans ~ asset('images/benefits-07.png') }}">
                        </div>
                        <span>{{ 'page.home.benefits.picto3'|trans|raw }}</span>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <a name="map"></a>
    <div class="container">
        <h3 class="title-wrap" id="searchHome">{{ 'page.home.map.search.title'|trans }}</h3>
        <div class="home-map-search-form">
            {% for element in map %}
                {% include ['mapping/_common/_properties/' ~ element ~ '.html.twig', 'mapping/_common/_properties/_' ~ element ~ '.html.twig'] %}
            {% endfor %}
        </div>
    </div>
</section>
