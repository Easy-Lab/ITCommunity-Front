    <script type="text/javascript">
        function margin(id) {
            var img = $("#pictures-" + id);
            if (img.height() < img.width()) {
                img.css('margin-top', '35%');
                {% if editable %}
                var pencil = $("#pencil-" + id);
                pencil.css('top', '30%');
                {% endif %}
            }
        }

        if ($(document).width() < 600) {
            $(document).ready(function () {
                var value = false;
                var nb = {{ nbPictures }};
                for (var i = 1; i <= nb; i++) {
                    var img = $("#pictures-" + i);
                    if (img.height() > img.width()) {
                        value = true;
                    }
                }
                if (value === true) {
                    for (var id = 1; id <= nb; id++) {
                        margin(id)
                    }
                }
            });
        }
    </script>
